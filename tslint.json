{
  "defaultSeverity": "error",
  "extends": ["tslint:all"],
  "jsRules": {},
  "rules": {
    "array-type": false,
    "comment-format": false,
    "completed-docs": false,
    "interface-over-type-literal": false,
    "max-line-length": [true, {"limit": 120, "ignore-pattern": "^import |^export {(.*?)}|'(.*?)'|\"(.*?)\"|`(.*?)`|"}],
    "member-access": false,
    "member-ordering": false,
    "newline-per-chained-call": false,
    "no-any": false,
    "no-implicit-dependencies": false,
    "no-magic-numbers": false,
    "no-parameter-properties": false,
    "no-require-imports": false,
    "no-shadowed-variable": false,
    "no-submodule-imports": [true, "rxjs"],
    "no-unbound-method": false,
    "no-unsafe-any": false,
    "object-literal-sort-keys": false,
    "only-arrow-functions": [
      true, "allow-declarations", "allow-named-functions"
    ],
    "quotemark": [
      true, "single", "avoid-template", "avoid-escape"
    ],
    "semicolon": [true, "always", "ignore-interfaces"],
    "strict-boolean-expressions": false,
    "typedef": false,
    "variable-name": [
      true, "ban-keywords", "check-format", "allow-leading-underscore"
    ]
  },
  "rulesDirectory": []
}
