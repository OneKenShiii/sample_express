{
  "name": "sample_express",
  "version": "0.1.0",
  "description": "simeple express application",
  "main": "server.js",
  "author": "kpping",
  "license": "MIT",
  "private": false,
  "scripts": {
    "start": "NODE_ENV=production node server.js",
    "dev": "NODE_ENV=development gulp",
    "build": "gulp build-ts-file",
    "compile": "tsc",
    "lint": "tslint -p tsconfig.json -t stylish",
    "test": "NODE_ENV=test jest"
  },
  "dependencies": {
    "@ag1/nil": "^0.1.1",
    "@ag1/return_switch": "^0.1.0",
    "body-parser": "^1.18.3",
    "boom": "^7.2.0",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "helmet": "^3.12.1",
    "http-status": "^1.2.0",
    "joi": "^13.4.0",
    "morgan": "^1.9.0",
    "rxjs": "^6.2.1",
    "winston": "^3.0.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/boom": "^7.2.0",
    "@types/cors": "^2.8.4",
    "@types/express": "^4.16.0",
    "@types/helmet": "^0.0.38",
    "@types/http-status": "^0.2.30",
    "@types/jest": "^23.1.1",
    "@types/joi": "^13.3.0",
    "@types/morgan": "^1.7.35",
    "@types/node": "^10.3.4",
    "gulp": "^4.0.0",
    "gulp-tslint": "^8.1.3",
    "gulp-typescript": "^5.0.0-alpha.2",
    "jest": "^23.1.0",
    "ts-jest": "^22.4.6",
    "tslint": "^5.10.0",
    "typescript": "^2.9.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.(ts)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/*.spec.+(ts)"
    ]
  }
}
